            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.unique_upgrade.ReliefPro.ChildcareActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.unique_upgrade.ReliefPro.HospitalsActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.unique_upgrade.ReliefPro.LabsActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.unique_upgrade.ReliefPro.DiseaseActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.unique_upgrade.ReliefPro.MainActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>