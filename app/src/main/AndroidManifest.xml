            android:name="com.unique_upgrade.ReliefPro.RegisterActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name="com.unique_upgrade.ReliefPro.SplashScreenActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

